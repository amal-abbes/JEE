CREATE TABLE ABONNE (IDENTITE INTEGER NOT NULL, NUMABONNE INTEGER, NOM VARCHAR(255), OUV_EMPR VARCHAR(255), PRIMARY KEY (IDENTITE))
CREATE TABLE Ouvrage (TITRE VARCHAR(255) NOT NULL, TYPE_OUV VARCHAR(31), DATECREATION VARCHAR(255), EXISTE TINYINT(1) default 0, EDITEUR VARCHAR(255), DUREE DOUBLE, AUTEUR VARCHAR(255), PRIMARY KEY (TITRE))
ALTER TABLE ABONNE ADD CONSTRAINT FK_ABONNE_OUV_EMPR FOREIGN KEY (OUV_EMPR) REFERENCES Ouvrage (TITRE)
